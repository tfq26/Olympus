# Frontend configuration (example)
# These are synced from root .env via scripts/sync-env.mjs.
# Override locally by editing Frontend/.env.local.

# Primary backend - Node MCP server (handles both infrastructure + monitoring proxy)
VITE_NODE_URL=http://localhost:8080
VITE_NODE_WS_URL=ws://localhost:8080

# Optional - Direct Flask access for monitoring endpoints
VITE_FLASK_URL=http://localhost:5000

# Firebase configuration (provide project values)
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=

# Add additional VITE_ prefixed vars here as needed.
