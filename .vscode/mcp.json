{
  "servers": {
    "terraform": {
      "type": "stdio",
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "--env-file",
        "${workspaceFolder}/server/.env",
        "-v",
        "${workspaceFolder}/mcps:/app",
        "mcps-terraform:dev"
      ]
    }
  }
}

